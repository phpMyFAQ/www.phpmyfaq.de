{
  "name": "phpmyfaq-website",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "cp -f static/.htaccess out/.htaccess || true",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:ci": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:e2e": "PLAYWRIGHT_TEST=1 playwright test",
    "test:e2e:ui": "PLAYWRIGHT_TEST=1 playwright test --ui",
    "test:e2e:headed": "PLAYWRIGHT_TEST=1 playwright test --headed",
    "fetch:versions": "npx tsx scripts/fetch-versions.ts",
    "fetch:downloads": "npx tsx scripts/fetch-downloads.ts",
    "update:data": "npm run fetch:versions && npm run fetch:downloads",
    "prepare": "husky",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@types/bootstrap": "^5.2.10",
    "@types/marked": "^5.0.2",
    "bootstrap": "^5.3.8",
    "gray-matter": "^4.0.3",
    "marked": "^16.4.2",
    "next": "15.5.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sass": "^1.94.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@next/eslint-plugin-next": "15.5.4",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "3.2.4",
    "conventional-changelog-atom": "^5.0.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^9.1.2",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
